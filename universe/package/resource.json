          ],
          "kind":"executable",
          "url":"{{artifact-dir}}/cli/darwin/dcos-kafka"
        }
      },
      "linux":{
        "x86-64":{
          "contentHash":[
            {
              "algo":"sha256",
              "value":"{{cli-linux-sha256}}"
            }
          ],
          "kind":"executable",
          "url":"{{artifact-dir}}/cli/linux/dcos-kafka"
        }
      }
    }
  }
